{
	"name":"jsx.UICalendar",
	"file":{
		"path":"../src/components/templates/jsx.UICalendar.jsx",
		"version":"0.1",
		"author":"zhang_7150@163.com",
		"description":"提供了基本的日历控件的[code]React Class[/code]模板",
		"updatetime":"2015-11-25"
	},
	"see":"../examples/calendar.html",
	"apis":{
		"Members":{
			"props":{
				"qualifier":"",
				"returnType":"Object",
				"default":{
					"date": "new Date",
					"currentDate":"new Date",
					"className":"",
					"hasHeader":true,
					"hasToolbar":true,
					"hasBody":true,
					"header":{
						"format":null
					},
					"body":{
						"fill":true,
						"renderItem":null,
						"createDates":null,
						"itemFormat":null,
						"select":null,
						"disabled":[],
						"weeks":[
							{
								"text":"日",
								"value":0,
								"className":"text-danger bg-danger"
							},{
								"text":"一",
								"value":1
							},{
								"text":"二",
								"value":2
							},{
								"text":"三",
								"value":3
							},{
								"text":"四",
								"value":4
							},{
								"text":"五",
								"value":5
							},{
								"text":"六",
								"value":6,
								"className":"text-danger bg-danger"
							}
						]
					},
					"toolbar":{
						"format":null,
						"years":[1900,2050],
						"hasToday":true,
						"todayText":"返回今日"
					}
				},
				"description":"组件的配置项",
				"parameters":{
					"date":{
						"type":"Date",
						"description":"当前日历加载的日期"
					},
					"currentDate":{
						"type":"Date",
						"description":"当前日历选中的日期"
					},
					"className":{
						"type":"string",
						"description":"日历模板容器要渲染的className，用于自定义样式"
					},
					"hasHeader":{
						"type":"boolean",
						"description":"日历是否加载Header部分"
					},
					"hasBody":{
						"type":"boolean",
						"description":"日历是否加载Body部分"
					},
					"hasToolbar":{
						"type":"boolean",
						"description":"日历是否加载Toolbar部分"
					},
					"header":{
						"type":"Object",
						"description":"日历头部部分的配置项",
						"parameters":{
							"format":{
								"type":"function",
								"arguments":"(date)",
								"returnType":"string",
								"description":"日历头部格式化器",
								"parameters":{
									"date":{
										"type":"Date",
										"description":"当前日历加载的日期"
									}
								}
							}
						}
					},
					"body":{
						"type":"Object",
						"description":"日历Body部分的配置项",
						"parameters":{
							"fill":{
								"type":"boolean",
								"description":"固定显示6行日历，不足的前后月份补充，默认为[code]true[/code]"
							},
							"disabled":{
								"type":"Array",
								"description":"被禁用的日期范围，如：[code][[,'2015-10-1'],['2015-11-12','2015-11-16'],['2015-12-31',]][/code]"
							},
							"weeks":{
								"type":"Array",
								"description":"星期配置项，顺序必须是周日~周六",
								"parameters":{
									"text":{
										"type":"string",
										"description":"显示的文字"
									},
									"value":{
										"type":"int",
										"description":"日期value值"
									},
									"className":{
										"type":"string",
										"description":"用于自定义的样式"
									}
								}
							},
							"itemFormat":{
								"type":"function",
								"description":"格式化日期对象，可向[code]item[/code]中注入自定义参数，在[code]select[/code]中被读取出来",
								"parameters":{
									"item":{
										"type":"Object",
										"description":"对日期对象的格式化",
										"parameters":{
											"year":{
												"type":"int",
												"description":"4位的年份"
											},
											"month":{
												"type":"int",
												"description":"当前月份"
											},
											"day":{
												"type":"int",
												"description":"当前日期"
											},
											"text":{
												"type":"string",
												"description":"cell中要渲染的文本，如果该值为空，则默认渲染[code]day[/code]"
											},
											"disabled":{
												"type":"boolean",
												"description":"当前日期是否被禁用，被禁用的日期无法被选中"
											}
										}
									},
									"returnValue":{
										"type":"Object",
										"description":"格式化后的日期对象"
									}
								}
							},
							"renderItem":{
								"type":"function",
								"description":"渲染日期cell的格式化函数",
								"parameters":{
									"item":{
										"type":"Object",
										"description":"当前日期",
										"parameters":{
											"year":{
												"type":"int",
												"description":"4位的年份"
											},
											"month":{
												"type":"int",
												"description":"当前月份"
											},
											"day":{
												"type":"int",
												"description":"当前日期"
											},
											"text":{
												"type":"string",
												"description":"cell中要渲染的文本，如果该值为空，则默认渲染[code]day[/code]"
											},
											"disabled":{
												"type":"boolean",
												"description":"当前日期是否被禁用，被禁用的日期无法被选中"
											}
										}
									},
									"returnValue":{
										"type":"string|ReactElement",
										"description":"返回要渲染到cell中的文本或[code]React[/code]组件"
									}
								}
							},
							"createDates":{
								"type":"function",
								"description":"生成日历组件中日期对象集合，[strong]不推荐用户改写该函数的行为[/strong]",
								"parameters":{
									"date":{
										"type":"Date",
										"description":"当前日历加载的日期"
									},
									"returnValue":{
										"type":"Object",
										"description":"返回生成的日期集合"
									}
								}
							},
							"select":{
								"type":"function",
								"description":"当日期被选中后的回调函数",
								"parameters":{
									"date":{
										"type":"string",
										"description":"当前选中日期字符串表示[code]2015-11-9[/code]"
									},
									"dateStyle":{
										"type":"Date",
										"description":"当前选中日期"
									},
									"item":{
										"type":"Object",
										"description":"当前选中的[code]item[/code]，可使用[code]itemFormat[/code]函数向[code]item[/code]中注入自定义参数"
									}
								}
							}
						}
					},
					"toolbar":{
						"type":"Object",
						"description":"工具栏[code]Toolbar[/code]的配置项",
						"parameters":{
							"years":{
								"type":"Array",
								"description":"当前日历支持的年份范围，如：[code][1900,2050][/code]"
							},
							"hasToday":{
								"type":"boolean",
								"description":"是否包含【返回今日】按钮"
							},
							"todayText":{
								"type":"string",
								"description":"【返回今日】按钮的文本"
							},
							"format":{
								"type":"function",
								"description":"工具栏年份和月份渲染时的格式化函数",
								"parameters":{
									"type":{
										"type":"string",
										"description":"当前渲染的年份还是月份，取值：[code]y,M[/code]"
									},
									"item":{
										"type":"int",
										"description":"当前渲染的年份或月份的值"
									},
									"returnValue":{
										"type":"string",
										"description":"返回需要渲染的文本"
									}
								}
							}
						}
					}
				}
			}
		}
	},
	"example":{
		"type":"javascript",
		"exec":true,
		"path":"../templates/exams/jsx.uicalendar.js"
	}
}