{
	"name":"Url",
	"file":{
		"path":"app/url.js",
		"version":"0.1",
		"author":"zhang_7150@163.com",
		"description":"url的代理类",
		"updatetime":"2015-11-25"
	},
	"apis":{
		"Members":{
			"hash":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"url中的hash字符串，#index"
			},
			"host":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"localhost:8089"
			},
			"hostname":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"localhost"
			},
			"href":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"http://localhost:8089/index.html?p=123#index"
			},
			"origin":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"http://localhost:8089"
			},
			"pathname":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"/index.html"
			},
			"relativepath":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"index.html"
			},
			"root":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"/，由 [link=application]Application[/link] 中配置的root"
			},
			"port":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"8089"
			},
			"protocol":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"http:"
			},
			"search":{
				"qualifier":"",
				"arguments":"",
				"returnType":"string",
				"description":"?p=123"
			}
		},
		"Instance Methods":{
			"param":{
				"qualifier":"",
				"arguments":"(name)",
				"returnType":"string",
				"description":"获取当前url中包含的参数",
				"parameters":{
					"name":{
						"type":"string",
						"description":"参数名"
					}
				}
			}
		},
		"Methods":{
			"constructor":{
				"qualifier":"constructor",
				"arguments":"(url)",
				"returnType":"Url",
				"description":"实例化Url对象",
				"parameters":{
					"url":{
						"type":"string",
						"description":"当前指定的url字符串"
					}
				}
			},
			"resolve":{
				"qualifier":"static",
				"arguments":"(relativePath)",
				"returnType":"string",
				"description":"解析当前url并返回一个绝对地址",
				"parameters":{
					"relativePath":{
						"type":"string",
						"description":"相对url地址"
					}
				}
			}
		}
	},
	"example":{
		"type":"javascript",
		"path":"url.js"
	}
}